{
  "name": "ai-agent",
  "displayName": "AI Assistant",
  "description": "Browser based AI agent",
  "version": "0.0.1",
  "publisher": "mrahat",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-agent.open",
        "title": "AI Assistant: Open Agent"
      }
    ],
    "chatParticipants": [
      {
        "id": "gemini-agent",
        "name": "Gemini Agent",
        "description": "Gemini AI Coding Assistant"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.106.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "diff": "^8.0.2",
    "socket.io-client": "^4.8.1"
  }
}
